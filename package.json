{
  "name": "@bhandarijiwan/js-actions-test",
  "version": "1.2.0",
  "description": "testing github actions",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "flow": "flow",
    "build:umd": "rollup --config",
    "build:src": "./node_modules/.bin/babel src -d dist --ignore 'src/**/*.test.js' ",
    "lint": "eslint './**/*.js'",
    "semantic-release": "semantic-release",
    "prepare": "npm run build:src && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bhandarijiwan/js-actions-test.git"
  },
  "keywords": [
    "github-actions"
  ],
  "files": [
    "dist",
    "umd"
  ],
  "author": "Jiwan Bhandari",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bhandarijiwan/js-actions-test/issues"
  },
  "homepage": "https://github.com/bhandarijiwan/js-actions-test#readme",
  "dependencies": {
    "dayjs": "1.8.16"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-flow": "7.0.0",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@semantic-release/git": "7.0.16",
    "babel-eslint": "10.0.3",
    "eslint": "6.4.0",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "flow-bin": "0.108.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "rollup": "1.21.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "semantic-release": "latest"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
